{
  "id": "Echo",
  "title": "Echo",
  "description": "This process accepts a Test input and returns an echo",
  "version": "1.0.0",
  "jobControlOptions": [
    "async-execute", "dismiss"
  ],
  "outputTransmission": [
    "value"
  ],
  "inputs": {
    "stringInput": {
      "title": "inputValue",
      "description": "The input value",
      "schema": {
        "type": "string"
      }
	}
  },
  "outputs": {
   "complexObjectOutput": {
      "title": "ouptputDocument",
      "description": "The output document",
      "schema": {
        "type": "object",
        "contentMediaType": "application/json",
        "required": [
          "result",
          "message"
        ],
        "properties": {
          "result": {
            "type": "string"
          },
          "message": {
            "type": "string"
          }
        }
        }
      }
  },
  "links": [
    {
      "href": "localhost:5000/processes/Echo/execution?input=test",
      "rel": "execute",
      "title": "Execute endpoint with test input"
    },
	  {
		  "href": "localhost:5000/processes/Echo?f=application/json",
		  "rel": "self",
		  "type": "application/json",
		  "title": "This document"
	  },
	  {
		  "href": "localhost:5000/processes/Echo?f=text/html",
		  "rel": "alternate",
		  "type": "text/html",
		  "title": "This document as HTML"
	  }
  ]
}
