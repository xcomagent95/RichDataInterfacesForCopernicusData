{
  "id": "Echo",
  "title": "Echo",
  "description": "This process accepts a Test input and returns an echo",
  "version": "1.0.0",
  "jobControlOptions": [
    "async-execute", "dismiss"
  ],
  "outputTransmission": [
    "value"
  ],
  "inputs": {
    "echo": {
      "title": "Value to be echoed",
      "description": "The input value",
      "schema": {
        "type": "string"
      }
	}
  },
  "outputs": {
